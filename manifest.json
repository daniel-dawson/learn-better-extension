{
  "name": "LearnBetter",
  "description": "Increase productivity and identify issues faster by showing open github issues, and hiding distracting text",
  "author": "Daniel Dawson",
  "version": "0.0.1",
  "manifest_version": 2,
  "default_locale": "en",
  "icons": {},
  "content_scripts": [
    {
      "matches": [
        "https://learn.co/tracks/*/*"
      ],
      "css": [
        "styles.css"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "background": {
    "scripts": [],
    "persistent": false
  },
  "permissions": [],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}